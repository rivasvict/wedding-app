{
  "name": "wedding-invitation",
  "version": "1.0.0",
  "description": "An app for wedding",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1,",
    "migrations": "knex --knexfile=src/server/knexfile migrate:latest",
    "seed-dev": "knex --knexfile=src/server/knexfile seed:run",
    "clean-db": "src/server/./cleandb.sh",
    "set-db": "npm run clean-db; npm run migrations; npm run seed-dev",
    "start": "nodemon src/server/app.js",
    "dev-setup": "chmod +x setup.sh; ./setup.sh",
    "use-node-version": "nvm use",
    "build-watch": "webpack --watch --config webpack.config.js",
    "compile-sass": "sass --watch src/client/styles/sass:src/client/styles/css",
    "babel-transpile": "babel --watch src/client/app -d src/client/precompiled-dist",
    "build-babel": "babel src/client/app -d src/client/precompiled-dist",
    "clean-temporal-directories": "scripts/./cleanTemporalDirectories.sh",
    "serve-dev": "run-p clean-temporal-directories babel-transpile compile-sass build-watch start",
    "babel": "babel src/client/app -d src/client/precompiled-dist",
    "sass": "sass src/client/styles/sass/index.scss src/client/styles/css/index.css src/client/styles/css/cover.scss",
    "build-webpack": "webpack --config webpack.config.js",
    "ugglify": "uglifyjs src/client/scripts.min.js --output src/client/scripts.min.js",
    "build": "run-s clean-temporal-directories babel sass build-webpack ugglify",
    "configure-db": "./configure-wedding-db.js",
    "configure-db-environment": "run-s configure-db clean-db set-db",
    "serve-production": "run-s build start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vctr90/wedding-app.git"
  },
  "keywords": [
    "wedding",
    "app"
  ],
  "author": "Victor Rivas",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/vctr90/wedding-app/issues"
  },
  "homepage": "https://github.com/vctr90/wedding-app#readme",
  "dependencies": {
    "axios": "^0.16.2",
    "babel": "^6.23.0",
    "body-parser": "^1.17.2",
    "bootstrap": "^3.3.7",
    "bunyan": "^1.8.10",
    "csvtojson": "^1.1.7",
    "dotenv": "^4.0.0",
    "expose-loader": "^0.7.3",
    "express": "^4.15.3",
    "file-loader": "^0.11.2",
    "font-awesome": "^4.7.0",
    "imports-loader": "^0.7.1",
    "jquery": "^3.2.1",
    "jquery-serializejson": "^2.8.1",
    "jquery.easing": "^1.4.1",
    "knex": "^0.13.0",
    "less": "^2.7.2",
    "lodash": "^4.17.4",
    "magnific-popup": "^1.1.0",
    "nodemailer": "^4.0.1",
    "npm-run-all": "^4.0.2",
    "pg": "^6.4.0",
    "query-string": "^4.3.4",
    "scrollreveal": "^3.3.6",
    "uglify-es": "^3.0.25",
    "uglify-js": "^3.0.25",
    "uglifyjs": "^2.4.11"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.5.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "before-build-webpack": "^0.1.0",
    "bootstrap-loader": "^2.1.0",
    "bootstrap-sass": "^3.3.7",
    "clean-dir": "^1.0.3",
    "css-loader": "^0.28.4",
    "node-sass": "^4.5.3",
    "nodemon": "^1.11.0",
    "resolve-url-loader": "^2.1.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "webpack": "^2.6.1"
  }
}
